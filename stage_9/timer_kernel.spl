print "TIMER 9";

//Assignment to print current PID of the program before returning to user mode

alias currentPID R0;
currentPID = [[SYSTEM_STATUS_TABLE + 1]];

print currentPID;

restore;

SP = [PROCESS_TABLE + ( [SYSTEM_STATUS_TABLE + 1] * 16) + 13];

ireturn;